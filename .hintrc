{
  "extends": [
    "development"
  ],
  "hints": {
    "no-inline-styles": "off",
    "button-type": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "speak",
          "user-select",
          "backdrop-filter",
          "text-wrap: nowrap",
